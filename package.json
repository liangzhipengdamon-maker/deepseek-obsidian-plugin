{
  "name": "deepseek-obsidian-plugin",
  "version": "1.0.0",
  "description": "Obsidian plugin for DeepSeek AI integration with knowledge base management",
  "main": "main.js",
  "types": "main.d.ts",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit && node esbuild.config.mjs production",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "deepseek",
    "ai",
    "knowledge-base"
  ],
  "author": "DeepSeek Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.2.0",
    "esbuild": "0.13.12",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
